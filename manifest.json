{
	"manifest_version": 2,
	"name": "Melhores Filtros para MelhoresDestinos.com.br",
	"short_name": "melhores-filtros",
	"description": "Para no tener que buscar entre toneladas de filas en una tabla cada vez que aparecen ofertas :)",
"version": "0.0.2",
"author": "Lucas Videla",
"homepage_url": "https://github.com/delucas/melhores-filtros",
"icons": {
	"48": "images/icon-48x48.png",
	"128": "images/icon-128x128.png"
	},
	"content_scripts": [
		{
			"js": [
				"jquery-1.11.3.min.js",
				"jquery.dataTables.min.js",
				"filtros.js"],
			"matches": ["*://*.melhoresdestinos.com.br/*"],
			"run_at": "document_idle"
		}
	],
	"permissions": [
  "activeTab"
]
} 